<root xmlns:nsd="http://structorizer.fisch.lu/" version="3.28-01" preRepeat="until " postFor="to" preReturn="return" postForIn="in" preWhile="while " output="OUTPUT" input="INPUT" preFor="for" preExit="exit" preLeave="leave" ignoreCase="true" preForIn="foreach" stepFor="by" author="schmuelling" created="2025-05-12" changedby="schmuelling" changed="2025-05-12" origin="Structorizer 3.28-01" text="&#34;DECLARE Kapital AS FLOAT&#34;,&#34;DECLARE Zinssatz AS FLOAT&#34;,&#34;DECLARE Laufzeit AS INT&#34;,&#34;DECLARE Jahr AS INT&#34;,&#34;DECLARE Zinsen AS FLOAT&#34;" comment="&#34;&#34;" color="ffffff" type="program" style="nice">
	<children>
